{
    "log": {
        "loglevel": "error"
    },
    "inbounds": [
    {{#select protocols "vmess"}}
        {
            "port": {{server.port}},
            "protocol": "{{name}}",
            "settings": {
                "clients": [
                    {{#each credentials}}
                     {
                         "id": "{{id}}",
                         "alterId": {{alterId}}
                     }
                     {{#unless @last}}
                     ,
                     {{/unless}}
                    {{/each}}
                 ]
            },
            "streamSettings": {
                "network": "tcp"
            }
         }
    {{/select}}
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
            }
        }
    ]
}
