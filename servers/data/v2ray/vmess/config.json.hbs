{
    "log": {
        "loglevel": "info"
    },
    "inbounds": [
    {{#protocol "vmess"}}
        {
            "port": {{server.port}},
            "protocol": "{{name}},
            "settings": {
                "clients": [
                    {{#each credentials}}
                     {
                         "id": "{{id}}",
                         "alterId": {{alterId}}
                     }
                     {{#unless @last}}
                     ,
                     {{/unless}}
                    {{/each}}
                 ]
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/v",
                    "headers": {
                        "host": "{{server.address}}"
                    }
                }
            }
         }
    {{/protocol}}
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
            }
        }
    ]
}
